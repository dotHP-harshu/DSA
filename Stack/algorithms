isFull:-
        -- Check if
            - Top is equal to -1 (top == -1)
                Return true (Stack is empty)
        -- else 
            -Return false( Stack is not empty)
        -- Exit

isFull:-
        -- Check if
            - Top is equal to MAX-1 (top == MAX-1)
                Return true (Stack is full)
        -- else 
            -Return false( Stack is not full)
        -- Exit

